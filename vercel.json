{
    "version": 2,
    "builds": [
        {
            "src": "index.html",
            "use": "@vercel/static"
        },
        {
            "src": "src/(.*)",
            "use": "@vercel/static"
        },
        {
            "src": "assets/(.*)",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "^/(?!.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|json)$).*$",
            "dest": "/index.html"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}